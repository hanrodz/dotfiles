[core]
	editor = vim
	pager = less -+X -F
[user]
	name = han rodr√≠guez
	email = hi@hanrodz.com
[alias]
	alias = config --global --get-regexp ^alias
	ln = log --oneline
	all = log --oneline --all --graph --decorate
	br = branch
	bra = branch -a
	brd = branch -D
	brm = branch -m
	ch = checkout
	chb = checkout -b
	cm = commit -m
	ca = commit --amend
	cam = commit --amend -m
	ne = commit --amend --no-edit
	cp = cherry-pick
	cpc = cherry-pick --continue
	df = diff
	dfn = diff --name-only
	dfs = diff --staged
	dfsn = diff --staged --name-only
	files = !f() { git diff HEAD HEAD~$1 --name-only; }; f
	pl = pull
	pla = pull --all
	pro = remote prune origin
	ps = push
	psf = push -f
	rb = rebase
	rba = rebase --abort
	rbc = rebase --continue
	rbi = rebase -i
	rs = reset
	rsh = reset --hard
	ss = stash
	ssa = stash apply
	ssd = stash drop
	ssl = stash list
	ssm = stash -m
	ssp = stash pop
	sss = stash show
	sssp = stash show -p
	st = status -s -b
[init]
	defaultBranch = main
[push]
	autoSetupRemote = true
